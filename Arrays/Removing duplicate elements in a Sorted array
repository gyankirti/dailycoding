#include <bits/stdc++.h>
using namespace std;

int main()
{
    vector<int> arr = {0,0,1,2,3,3,3,4,5,6,6};
    int i = 0 , j = 1;
    while(j < arr.size()){
        if(arr[i]!=arr[j]){
            i++;
            arr[i] = arr[j];
        }
        j++;
    }
    for(int i = 0 ; i < arr.size() ; i++){
        cout<<arr[i]<<" ";
    }
}
